 DELETE XT_PC_PCX where MRZ='164数据';
 DELETE XT_PC_PCXFL WHERE PCJLBM LIKE '%164数据%';
 DELETE XT_PC_MB where SM='164数据';


SELECT PCMBBM FROM XT_PC_MB where sm='0504cq';
SELECT * FROM XT_PC_PCXFL WHERE PCJLBM LIKE '%0504cq%' ;
